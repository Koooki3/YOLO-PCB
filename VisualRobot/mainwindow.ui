/* Modern minimal dark theme */
QMainWindow, QWidget {
    background: #0f1720;
    color: #e6eef6;
    font-family: "Segoe UI", "Microsoft YaHei", Arial;
    font-size: 11pt;
}

/* Containers */
QGroupBox { 
    border: 1px solid rgba(255,255,255,0.06);
    border-radius: 8px;
    margin-top: 8px;
    padding: 10px;
    background: rgba(255,255,255,0.012);
}

/* Buttons */
QPushButton {
    background: qlineargradient(x1:0,y1:0,x2:0,y2:1, stop:0 #16a6d9, stop:1 #0b8fbf);
    color: white;
    border: none;
    border-radius: 6px;
    padding: 8px 12px;
    min-height: 34px;
}
QPushButton:disabled {
    background: rgba(255,255,255,0.03);
    color: rgba(255,255,255,0.4);
    border: 1px solid rgba(255,255,255,0.02);
}
QPushButton:hover { filter: brightness(1.06); }
QPushButton:pressed { transform: translateY(1px); }

/* Flat secondary buttons */
QPushButton[flat="true"] {
    background: transparent;
    color: #9fbddc;
    min-height: 30px;
}

/* Input fields */
QLineEdit, QPlainTextEdit, QTextEdit {
    background: #0b1220;
    border: 1px solid rgba(255,255,255,0.06);
    border-radius: 6px;
    padding: 6px;
    color: #dbeefe;
}
QLineEdit:disabled { color: rgba(255,255,255,0.3); }

/* Combo / Spin */
QComboBox, QSpinBox, QDoubleSpinBox {
    background: #0b1220;
    border: 1px solid rgba(255,255,255,0.06);
    border-radius: 6px;
    padding: 4px 6px;
    color: #e6eef6;
}

/* Log view */
#displayLogMsg, QPlainTextEdit {
    background: #071018;
    border: 1px solid rgba(255,255,255,0.04);
    color: #bfe8ff;
    border-radius: 6px;
}

/* Status bar */
QStatusBar {
    background: transparent;
    color: #9fbddc;
    border-top: 1px solid rgba(255,255,255,0.02);
}

/* Camera preview placeholder */
QLabel#widgetDisplay, QLabel#widgetDisplay_2 {
    background: #061018;
    border: 1px dashed rgba(255,255,255,0.04);
    border-radius: 6px;
}

/* Small helpers */
QLabel { color: #bcd7ea; }
QCheckBox, QRadioButton { color: #cfeffd; }
